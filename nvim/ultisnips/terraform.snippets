snippet var "variable"
variable "${1:name}" {
	${1}
}
endsnippet

snippet provider
provider "${1:name}" {
	${2:config}
}
endsnippet

snippet resource
resource "${1:type}" "${2:name}" {
	${3:config}
}
endsnippet

snippet output
output "${1:name}" {
	value = "${2:${aws_instance.foo.public_ip}}"
}
endsnippet
